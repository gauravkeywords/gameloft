      "parameters": {
        "jsCode": "const mappings = {\n  'vectordb': {\n    table_name: 'documents',\n    query_filter: 'match_documents'\n  }\n};\n\nconst items = $input.all();\n\nfor (const item of items) {\n  // Safe access to model name, defaults to empty string if missing\n  const modelName = item.json.body.metadata?.model?.name?.toLowerCase() || '';\n  \n  // Default result structure - REMOVED studio_name\n  let result = { \n    table_name: null, \n    query_filter: null \n  };\n  \n  for (const [ending, mapping] of Object.entries(mappings)) {\n    if (modelName.endsWith(ending)) {\n      result = mapping;\n      break;\n    }\n  }\n  \n  item.json = result;\n}\n\nreturn items;"
      },
